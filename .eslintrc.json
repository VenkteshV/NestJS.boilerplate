{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "document": true,
        "describe": true,
        "beforeEach": true,
        "context": true,
        "expect": true,
        "it": true,
        "window": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "@typescript-eslint/eslint-plugin"
    ],
    "rules": {
        "no-unused-vars": 0,
        "nonblock-statement-body-position": 0,
        "no-useless-constructor": 0,
        "no-empty-function": 0,
        "lines-between-class-members": 0,
        "func-names": 0,
        "prefer-arrow-callback": 0,
        "import/first": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "class-methods-use-this": 0,
        "import/extensions": 0
    }
}